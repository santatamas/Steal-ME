<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- This variable determines the number of 
         SocketAsyncEventArg objects put in the pool of objects for receive/send.
         The value of this variable also affects the Semaphore.
         This app uses a Semaphore to ensure that the max # of connections
         value does not get exceeded.
         Max # of connections to a socket can be limited by the Windows Operating System also.-->
    <add key="maxNumberOfConnections" value="5000" />
    
    <!--Port the server listens on-->
    <add key="port" value="4444" />

    <!--IP Address the server listens on - comment out to use IPAdress.Any-->
    <!--<add key="endpointAdr" value="192.168.0.105" />-->

    <!-- Default buffer size for Socket operations -->
    <!-- CANNOT BE SMALLER THAN PREFIX SIZE! -->
    <add key="bufferSize" value="50" />

    <!--This is the maximum number of asynchronous accept operations that can be 
        posted simultaneously. This determines the size of the pool of 
        SocketAsyncEventArgs objects that do accept operations. Note that this
        is NOT the same as the maximum # of connections.-->
    <add key="maxSimultaneousAcceptOps" value="500"/>

    <!--The size of the queue of incoming connections for the listen socket.-->
    <add key="backLog" value="15"/>

    <!--Tell the BufferManager to occupy 2 buffer for each transport channel (separate send/receive SAEA) -->
    <add key="opsToPreAlloc" value="2"/>

    <!--allows excess SAEA objects in pool. -->
    <add key="excessSaeaObjectsInPool" value="1"/>

    <!--This number must be the same as the value on the client.
        Tells what size the message prefix will be. Don't change this unless
        you change the code, because 4 is the length of 32 bit integer, which
        is what we are using as prefix.
        !!!!MUST NOT BE BIGGER THAN THE BUFFER SIZE!!! -->
    <add key="receivePrefixLength" value="4"/>
    <add key="sendPrefixLength" value="4"/>
  </appSettings>
  <connectionStrings>
    <add name="StealMEEntities" connectionString="metadata=res://*/DAL.stealme_db.csdl|res://*/DAL.stealme_db.ssdl|res://*/DAL.stealme_db.msl;provider=System.Data.SqlClient;provider connection string='data source=.\SQLEXPRESS;initial catalog=StealME;integrated security=True;multipleactiveresultsets=True;App=EntityFramework'" providerName="System.Data.EntityClient" />
  </connectionStrings>
</configuration>
